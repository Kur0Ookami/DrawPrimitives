<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.aermatov.drawcanvas" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".DrawPrimitives"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Rectangle"
            android:label="@string/title_activity_rectangle" >
        </activity>
        <activity
            android:name=".Circle"
            android:label="@string/title_activity_circle" >
        </activity>
        <activity
            android:name=".Image"
            android:label="@string/title_activity_image" >
        </activity>
        <activity
            android:name=".Line"
            android:label="@string/title_activity_line" >
        </activity>
    </application>

</manifest>
